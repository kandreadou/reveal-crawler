        currentView = 'web';

        function togglePubs() {
            if (currentView != detectCurrentView()) {
                currentView = detectCurrentView();

                if (detectCurrentView() == 'tablet') {
                    var strAppendSource = '';
                    var strAppendTarget = '_tablet';
