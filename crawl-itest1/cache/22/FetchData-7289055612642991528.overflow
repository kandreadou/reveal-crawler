				displaySlideQty: nbItems,
				moveSlideQty: nbItems,
				infiniteLoop: false
			});

			$(".bx-pager").addClass("clearfix");
		}

		$(function(){
			setupCarrousel();
		});

		// On window resize, refresh the carrousel so it renders correctly
		var rtime = new Date(1, 1, 2000, 12,00,00);
		var timeout = false;
		var delta = 200;

		$(window).resize(function() {
			rtime = new Date();
			if (timeout === false) {
				timeout = true;
				setTimeout(resizeEnd, delta);
			}
		});

		function resizeEnd() {
			if (new Date() - rtime < delta) {
				setTimeout(resizeEnd, delta);
			} else {
				timeout = false;
				setupCarrousel();
			}
		}

	</script>

