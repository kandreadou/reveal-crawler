                    $('#' + pubZones[zone] + strAppendSource).children('div:first').remove();

                    clone.appendTo($('#' + pubZones[zone] + strAppendTarget));
                }
            }
        }

        windowObject.resize(function(){
            if (!($.browser.msie && ($.browser.version < 10))) {
                togglePubs();
            }
        });

