			}
			else {
				iframe_src = 'http://ko.snstatic.fi/is-sale-kysa/';
			}
            
        } else if (geoip.county == 'Etelä-Karjala') {
            iframe_src = 'http://ko.snstatic.fi/is-sale-esaimaa/';
        }

        if (iframe_src != '') {
            $('#geouutiset').html($('<iframe />', {"name":"geoiframe","id":"geoiframe","src":iframe_src,"height":375,"width":658,"scrolling":"no","border":0,"frameborder":0}));
        }
    }
});
