			 */
			function findItemByName(name, jsonData) {
				for (var i=0;i < jsonData.ITEM.length; i++) {
					var curItem = jsonData.ITEM[i];
					for (var j=0;j < curItem.DATA.length; j++) {
						var curField = curItem.DATA[j];
