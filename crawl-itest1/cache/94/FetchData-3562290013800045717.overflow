	var lastChar = dani.substr(dani.length - 1);

	var dana = 'dana';
	if( lastTwo != '11' && lastChar == '1' ){
		dana = 'dan';
	}

	$('#days').text( dana );
}

function setCountDown () {
	$('#countdown').countdown({until:$.countdown.UTCDate(+1, 2015,  3 - 1, 2, 23, 59, 59), format: 'DHMS', layout:
		'{dnn}',

		onExpiry: function(){},
		onTick: watchCountdown
	});
}

