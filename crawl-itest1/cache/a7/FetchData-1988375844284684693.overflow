pol.lazyload = function(force){
if ($p(window).scrollTop() > pol.lazyScrollPos || force) {
pol.lazyScrollPos = $p(window).scrollTop();
$p('.lzy').each(function(){
if(pol.lazyScrollPos + $p(window).height() + 450 > $p(this).offset().top){
var data = $p(this).data();
if($p(this).prop('tagName') == 'IFRAME'){
$p(this).height(data.height).attr('src',data.src);
}else{
$p(this).attr('src',data.src);
}
$p(this).addClass('lzy-done').removeClass('lzy');
}
});
if($p('.lzy').length == 0){
clearInterval(pol.lazyChecker);	
}
}
}
pol.initLazyLoad = function(){
pol.lazyScrollPos = 0;
pol.lazyChecker = setInterval(function() {
pol.lazyload(false);
}, 200);	
}
pol.initLazyLoad();
