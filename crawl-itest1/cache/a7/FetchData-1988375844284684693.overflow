function simpleEscUrlParser(url){
var hostRE = /\/\/([\w\.]*)/i;
var pathRE = /^(?:[^\/]*(?:\/(?:\/[^\/]*\/?)?)?([^?]+\/)(?:\??.+)?)$/;
var escIdRE = /\/ece(\d*)\//i;
var PageTitleRE = /ece\d*\/([\w-]*)/i;
var ibyenIdRE = /ibyen\/vis\/(\d*)/i;
var host = getMatch(hostRE);
var path = getMatch(pathRE);
var pathSegments = path === null ? [] : path.split('/');
function getMatch(regex){
var matches = regex.exec(url);
return matches === null ? null : matches[1];
}
return {
host: host,
pagetitle: getMatch(PageTitleRE),
escId: getMatch(escIdRE),
ibyenId: getMatch(ibyenIdRE),
section: host === "politiken.dk" ? pathSegments[0] : "external",
subsection: typeof pathSegments[1] !== "undefined" && !/^ece/i.test(pathSegments[1]) ? pathSegments[1] : null
}
}
