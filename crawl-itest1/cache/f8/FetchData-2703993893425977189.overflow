<script type="text/javascript">
    var $radios = $('#achat_express input[type=radio]');
    var $all_lis = $('#achat_express li') ;     
    $(document).ready(function() {
        $('#lnk_all_regions').click(bloc_achat_toggle_regions);
        bloc_achat_region_clicked();
        
        $('#achat_btn_comparer').click(function(e){
            $('#achat_regions').removeClass('unfold');
            bloc_achat_bind_comparer();
            e.preventDefault();
        });        
        bloc_achat_default_radio();
        
        if($.browser.msie == false){
            bloc_achat_update_radio_class();
            $('#achat_express input[type="checkbox"],#achat_express input[type="radio"]').checkbox();
            $('#achat_express input[type="checkbox"],#achat_express input[type="radio"]').change(bloc_achat_update_radio_class);            
        }
        $('#achat_regions input').click(bloc_achat_region_clicked);
    });
    
    
    
    /* mise  jour des tiquettes radio en rouge */
    function bloc_achat_update_radio_class() {
        if( typeof this.id != 'undefined'){
            id =  this.id ; 
            if( id && id.indexOf('region_') == 0){
                return;
            }
                 
        }
        $all_lis.removeClass('active');
        $radios.each(function(){
            if( $(this).is(':checked')){
                $($(this).closest('li')).addClass('active');
            }
        });
        $('#achat_regions').removeClass('unfold')            
        
    }    

    function bloc_achat_toggle_regions() {
        if( $('#achat_regions').hasClass('disabled') == false){
            $('#achat_regions').toggleClass('unfold');
            $('#achat_express .buttons input').toggle();
            if($.browser.msie == true && $('#achat_regions').hasClass('unfold')){
                $('#tabsmain-selectwine').hide();
            }else{
                $('#tabsmain-selectwine').show();
            }
        }
    }

    function bloc_achat_region_clicked() {
        nb = $('#achat_regions input:checked').length;
        if( nb == 0 ){
            txt = 'toutes les rgions';
        }else if(nb == 1){
            txt = '1 rgion';
        }else{
            txt = nb + ' rgions';
        }
        $('#lnk_all_regions').html(txt);
    }
    
    function bloc_achat_default_radio() {
        if( $('#achat_prix input:checked').length == 0)
            $('#achat_prix_2').prop('checked',true);

        if( $('#achat_couleurs input:checked').length == 0)
            $('#achat_couleur_rouge').prop('checked',true);
    }
    
