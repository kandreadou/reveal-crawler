    <script type="text/javascript">
        var date = new Date(2014, 12-1, 10);
        function navigate(d)
        {
            document.location = "#" + d.getDate() + "-" + d.getMonth() + "-" + d.getFullYear();
        }

        $(document).ready(function(){
            resetCoverNavigation();
            $(window).bind('hashchange', function () {
                getCovers();
            });

            if(window.location.hash)
            {
                getCovers();
            }
        });

        function resetCoverNavigation()
        {
            $(".prev").on("click", function(){
                var today = new Date();
                today.setDate(date.getDate() - 1);
                navigate(today);
                return false;
            });
            $(".next").on("click", function(){
                var today = new Date();
                today.setDate(date.getDate() + 1);
                navigate(today);
                return false;
            });        
        }

        function getCovers()
        {
            var pieces = window.location.hash.replace("#", "").split("-");
            var newDate = new Date(pieces[2], pieces[1]-1, pieces[0]);
            $(".newspaper-cover-container time").html(newDate.getDate() + " de " + MonthName[newDate.getMonth()] + " de " + newDate.getFullYear());
            LoadJsonData("cover-holder", [{ DataInicio: newDate.toGMTString() }], function (d) {
                if(d && d.length > 0){
                    $("#thumbnails").html($("#thumbnailTemplate").tmpl({data: d}));
                    $("#thumbnails li").addClass("active");
                }
            }, null, 'GET');
            date = newDate;        
        }
    </script>

</div>

