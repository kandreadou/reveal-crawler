		<script type="text/javascript">
			$(".overflowLinks").each(function(){
				var menuWidth = $(this).width();
				var menuLength = $("li",this).length;
				var elementWidth = 0;
				var bigWidth = 0;
				var overflowIndex = 0;
				$("li",$(this)).each(function(){
					elementWidth = elementWidth+((parseInt($(this).css("width"))+(parseInt($(this).css("padding-right"))+parseInt($(this).css("padding-left")))+(parseInt($(this).css("margin-right"))+parseInt($(this).css("margin-left")))));
					if(((parseInt($(this).css("width"))+(parseInt($(this).css("padding-right"))+parseInt($(this).css("padding-left")))+(parseInt($(this).css("margin-right"))+parseInt($(this).css("margin-left")))+(parseInt($(this).css("border-right"))+parseInt($(this).css("border-left")))))>bigWidth){
						bigWidth = ((parseInt($(this).css("width"))+(parseInt($(this).css("padding-right"))+parseInt($(this).css("padding-left")))+(parseInt($(this).css("margin-right"))+parseInt($(this).css("margin-left")))+(parseInt($(this).css("border-right"))+parseInt($(this).css("border-left")))));
					}
				});
				//menuWidth = menuWidth - bigWidth;
				if(menuWidth<elementWidth){
					elementWidth = 0;
					$("li:first-of-type",$(this)).before('<li class="more"><strong>ุงููุฒูุฏ</strong><ul class="RLS"></ul></li>');
					$("li",$(this)).each(function(){
						elementWidth = elementWidth+((parseInt($(this).css("width"))+(parseInt($(this).css("padding-right"))+parseInt($(this).css("padding-left")))+(parseInt($(this).css("margin-right"))+parseInt($(this).css("margin-left")))));
						if(menuWidth<elementWidth){
							if(overflowIndex==0){
								overflowIndex = $(this).index();
							}
						}
					});
					if(overflowIndex!=0){
						for(var i=(overflowIndex-1);i<=menuLength;i++){
							$("li:eq("+i+")",$(this)).removeClass("col").appendTo($(".more ul",$(this)));
						}
						$(this).css({"overflow":"visible"});
					}
				}
			});
		</script>		
