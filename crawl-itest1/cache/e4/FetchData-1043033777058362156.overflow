		<script type="text/javascript">
			var currentSlide = 1;
			var slidesCount = $(".slider .slides .slide").length;
			var sliderAutoPlayTimeing;
			function showInMainSlide(theindex){
				$(".slider .slide").removeClass("active");
				$(".slider .slide:eq("+(theindex-1)+")").addClass("active");
				$(".slider .mainSlide .image").attr("href",$(".slider .slide:eq("+(theindex-1)+")").attr("link"));
				$(".slider .mainSlide .title").attr("href",$(".slider .slide:eq("+(theindex-1)+")").attr("link"));
				$(".slider .mainSlide .image img").attr("src","http://moheet.com/application/themes/default/img/loading.gif");
				$(".slider .mainSlide .image img").attr("src",$(".slider .slide:eq("+(theindex-1)+")").attr("path"));
				$(".slider .mainSlide .title").html($(".slider .slide:eq("+(theindex-1)+")").attr("description"));
				$(".gallery .count").html(slidesCount+" / "+theindex);
			}
			function slidesAutoPlayFunction(){
				showInMainSlide(currentSlide);
				currentSlide = ((currentSlide+1)>slidesCount)?1:(currentSlide+1);
				sliderAutoPlayTimeing = setTimeout(function(){slidesAutoPlayFunction()},10000);
			}
			slidesAutoPlayFunction();
			$(".slider .slide").click(function(){
				clearTimeout(sliderAutoPlayTimeing);
				currentSlide = ($(".slider .slide").index($(this))+1);
				slidesAutoPlayFunction();
			});
		</script>
