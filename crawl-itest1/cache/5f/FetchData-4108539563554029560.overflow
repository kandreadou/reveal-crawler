<script language="javascript" type="text/javascript">
    UR_Start();
    function UR_Start() {
        UR_Nu = new Date()
        var weekday = new Array("Sunnuntai", "Maanantai", "Tiistai", "Keskiviikko", "Torstai", "Perjantai", "Lauantai")

        dow = weekday[UR_Nu.getDay()];
        dd = UR_Nu.getDate() + "." + (UR_Nu.getMonth() + 1) + "." + UR_Nu.getFullYear();
        dtime = showFilled(UR_Nu.getHours()) + ":" + showFilled(UR_Nu.getMinutes());

        document.getElementById("dayofweek").innerHTML = dow;
        document.getElementById("daydate").innerHTML = dd;
        document.getElementById("daytime").innerHTML = dtime;

        setTimeout("UR_Start()", 1000);
    }
    function showFilled(Value) {
        return (Value > 9) ? "" + Value : "0" + Value;
    }
