<script type="text/javascript">

(function initCommenting() {
    hs.commenting = {};

    // Same commenting logic is used for various content types. Determine 
    // the proper values for current page:
    var baseUrl = "/ajax/keskustelut/1305682354296/";
    if (hs.article && hs.article.current) {
        hs.commenting.ajaxUrl = baseUrl + "a" + hs.article.current.id;
    } else if (hs.video && hs.video.current) {
        hs.commenting.ajaxUrl = baseUrl + "v" + hs.video.current.id;
    } else if (typeof newComicId !== 'undefined') {
        hs.commenting.ajaxUrl = baseUrl + "s" + newComicId;
    }
})();

$(function () {

    var order = $.cookie("_hs_comments_order");
    
    var url = hs.commenting.ajaxUrl;
    if (order) {
        url = url + "?order=" + order;
    }

    $("#discussions-list").load(url, null, function () {
        UserProfile.refreshCommentIcons();
        if (typeof ArticleCommentTotCount !== 'undefined') {
            $('.header > h3 > span').text("(" + ArticleCommentTotCount + ")");
            if (ArticleCommentTotCount > 0) {
                $('#kommentit .add-first-comment').addClass('hidden');
            }
        }
        $("#discussions").show();
        $('.star').rating().rating('readOnly', true);
    });
    $("#reportAbusiveCommentContainer .cancel").click(function () {
        $("#reportAbusiveCommentContainer").hide();
        return false;
    });

    initializeCommentTools();

    var form = IFrameForms.init("#frm-report-abusive-comment", "iframe-frm-report-abusive-comment", function () {
        var valid = $("#frm-report-abusive-comment").validate({
                    messages:{
                        text: { required:"Kuvaile tekstikenttään miksi viesti on asiaton" }
                    },
                    rules:{
                        text:{ required:true }
                    }
                }).form();
        return valid;
    });

    form.bind(
        function () {
            showAbuseReportSentMessage("Viesti lähetetty, kiitos ilmoituksesta.");
            enableAbuseReportPrinting();
        },
        function (form, result) {
            showAbuseReportSentMessage("Virhe viestin lähetyksessä. Yritä myöhemmin uudelleen.");
        }
    );
});

function showAbuseReportSentMessage(message) {
    $("#reportAbusiveCommentContainer .buttons").hide();
    $("#reportAbusiveCommentContainer textarea").attr("disabled", "disabled");
    $("#abuse-report-sent-message").text(message).show();
}

function enableAbuseReportPrinting() {
    $("#abuse-report-print-message").text("Voit halutessasi tulostaa vahvistuksen ilmoittamastasi asiattomasta viestistä.").show();
    $("#reportAbusiveCommentContainer .printButton").show();
}

function initializeCommentTools() {
    $('body').on("click", "#commenting .add-comment, #commenting .add-first-comment", function () {
        $('#form-send-comment').removeClass('hidden');
        return false;
    });

    $('body').on("click", "#commenting .show-more", function () {
        var startIndex = $("#comment-list-container ul").attr("last-index");
        var order = "a";
        if (!$("#commenting .show-latest").is(":visible")) {
            order = "d";
        }
        var oldMessages = $("#comment-list-container ul").html();
        var args = {
            "startIndex":startIndex,
            "order":order
        };
        var url = hs.commenting.ajaxUrl + "?" + $.param(args);

        var callback = function (responseText) {
            $("#comment-list").prepend(oldMessages);
            UserProfile.refreshCommentIcons();
            $('.star').rating().rating('readOnly', true);
        };

        $("#discussions-list").load(url, null, callback);

        hideOldSendMessages();
        //$("#kommentit .scroll-to-comment").show();
        Metrix.track("HS-OmaHS", "artikkelisivu-kommentti-nayta");
        //$('#form-send-comment').removeClass('hidden');
        return false;
    });

    $('body').on("click", ".comment-ftr .comment-report a", function () {
        var link = $(this);
        var commentId = link.attr("comment-id");
        if (commentId == null) {
            return false;
        }

        $("#reportAbusiveCommentContainer textarea")
                .removeAttr("disabled"); //This must be done beforfe the form is detached from DOM
        var container = $("#reportAbusiveCommentContainer")
                .detach();
        container.find("input[name=commentId]").val(commentId);
        container.find("textarea").val("");
        container.find("#abuse-report-sent-message").text("")
                .hide();
        container.find("#abuse-report-print-message").text("")
                .hide();
        container.find(".printButton").hide();
        container.find(".buttons").show();

        link.closest(".comment-ftr").closest("li").children(".comment-text").children(".reply-btn-group").append(container);
        $("#reportAbusiveCommentContainer textarea").attr(
                "placeholder",
                "Kuvaile tähän miksi viesti on asiaton");
        hs.security.initCsrfToken(function(csrfToken) {
            $(container).find("input[name=csrfToken]").val(csrfToken);
            container.show();
        });
        return false;
    });

    $('body').on("click", ".comment-text .reply-btn-group .comment-reply a", function () {
        var link = $(this);
        var commentId = link.attr("comment-id");
        if (commentId == null) {
            return false;
        }

        $("#form-send-comment input[name=replyToMessageId]").val(commentId);

        var repliedUserText = $("#comment-meta" + commentId + " a.author").text();

        if (repliedUserText == null || repliedUserText.length == 0) {
            repliedUserText = $("#comment-meta" + commentId + " span:first").text();
        }

        showReplyToMessage(repliedUserText);

        return false;
    });

    $('body').on("click", ".comment-text-links .print", function () {
        var content = $(this).closest(".comment-text").parent().clone();
        content.find(".comment-text-links").remove();
        showAbusiveCommentReceipt(content.html());
        return false;
    });

    $('#kommentit').on("click", ".login-link", function (event) {
        event.preventDefault();
        UserProfile.hugeSignIn();
        return false;
    });

    $('#kommentit').on("click", ".scroll-to-comment", function (event) {
        event.preventDefault();
        $.scrollTo("#form-send-comment", {
            axis:"y",
            duration:200
        });
        return false;
    });

    $("body").on("click", ".comment-recommend", function (event) {
    	Metrix.track("HS-OmaHS", "artikkelisivu-kommentti-arvostelu");
    	if (UserProfile.userDetails == null || UserProfile.userDetails.isActivated != "true") {
            event.preventDefault();
            UserProfile.hugeSignIn();
    		return;
    	}
        var articleId = $(this).attr('aId');
        var commentId = $(this).attr('mId');
        var disabled = $(this).attr('this');
        var code = parseInt($(this).attr('code'));
        var recommended = $.cookie("recommended" + ((code == 11 || code == 12 ? "agreed" : ((code == 13 || code == 14) ? "agrument" : "else"))));

        if (!$(this).hasClass('disabled-comment-btn') && (recommended === null || recommended === undefined || recommended.search("\\b" + commentId + "\\b") < 0)) {
            //this is entered on hyvin kommentoitu kylla/ei
            var recommendations = $.cookie("recommended" + ((code == 11 || code == 12 ? "agreed" : ((code == 13 || code == 14) ? "agrument" : "else"))));
            if (recommendations) {
                recommendations = recommendations + "|" + commentId;
            } else {
                recommendations = commentId;
            }
            $.cookie("recommended" + ((code == 11 || code == 12 ? "agreed" : ((code == 13 || code == 14) ? "agrument" : "else"))), recommendations, {expires:2, path:window.location.pathname});
            hs.security.initCsrfToken(function(csrfToken) {
                $.ajax({url:"/ajax/keskustelut/aanesta", data:{"articleId":articleId, "commentId":commentId, "code":code, "csrfToken": csrfToken}, type:"POST",
                    success: function (data, status, jqXHR) {
                        var btnText = $("#recommendButton" + commentId + "-" + code).html();
                        var clicks = btnText.match(/\d+/);
                        btnText = btnText.replace(clicks, ++clicks);
                        $("#recommendButton" + commentId + "-" + code).html(btnText);
                    }
                });
                disableCommentButtons(commentId, code);
            })
        } else {
            //this is entered on agree/disagree
        	disableCommentButtons(commentId, code);
        }
        return false;
    });
    
    function disableCommentButtons(commentId, code) {
        var even = code % 2 == 0;
        $("#recommendButton" + commentId + "-" + code).removeClass("comment-recommend");
        $("#recommendButton" + commentId + "-" + code).addClass("disabled-comment-btn");
        $("#recommendButton" + commentId + "-" + (even ? (code-1) : (code+1))).removeClass("comment-recommend");
        $("#recommendButton" + commentId + "-" + (even ? (code-1) : (code+1))).addClass("disabled-comment-btn");
    }


    $("body").on("click" , ".show-latest", function() {
        $.cookie("_hs_comments_order", "d", {expires:7, path:window.location.pathname});
        var path = hs.commenting.ajaxUrl;
        
        $.ajax({url: path + "?order=d", type:"POST",
            success: function (data, status, jqXHR) {
            	$("#discussions-list").html(data);
                UserProfile.refreshCommentIcons();
                $('.star').rating().rating('readOnly', true);
            }
        });
        
        $(this).hide();
        $(".show-oldest").show();
        
        return false;
    });

    $("body").on("click", ".show-oldest", function() {
        var path = hs.commenting.ajaxUrl;
        
        $.cookie("_hs_comments_order", "a", {expires:7, path:window.location.pathname});
        $.ajax({url: path + "?order=a", type:"POST",
            success: function (data, status, jqXHR) {
            	$("#discussions-list").html(data);
                UserProfile.refreshCommentIcons();
                $('.star').rating().rating('readOnly', true);
            }
        });
        $(this).hide();
        $(".show-latest").show();
        
        return false;
    });

}
</script>



