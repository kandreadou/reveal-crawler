        }
        
        var charactersLeft = 2000 - CommentForm.textarea.val().length;
        if (charactersLeft == 2000) {
        	CommentForm.charactersLeftInfo.html("Maksimi merkkimäärä on 2000");         
        } else {
        	CommentForm.charactersLeftInfo.html(charactersLeft + " merkkiä jäljellä");
        }
    }
}

