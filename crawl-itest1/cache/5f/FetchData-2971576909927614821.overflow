    updateCharactersLeftInfo: function() {
        if (CommentForm.textarea.val().length > 2000) {
        	CommentForm.textarea.val(CommentForm.textarea.val().substring(0, 2000));
            CommentForm.textarea.caretToEnd();
        }
        
        if(CommentForm.textarea.val().length > 0) {
        	 $("#form-send-comment .comment-btn").removeAttr("disabled");
        } else {
