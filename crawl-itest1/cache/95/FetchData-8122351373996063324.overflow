<script type="text/javascript">
    function eBultenUyeKayit() {
        if ($.trim($("#txtAdSoyadForEbulten").val()) == "" || $.trim($("#txtEpostaForEbulten").val()) == "") {
            eBultenShowBox('Lütfen tüm alanları doldurunuz.', 'uyari', 2500);
        }
        else {
            $.ajaxSetup({ cache: false });
            $.getJSON(unescape('/eBulten/UyeKayit/' + $("#txtAdSoyadForEbulten").val() + '/' + $("#txtEpostaForEbulten").val()), null,
            function (data) {
                if (data.Success) {
                    $("#btnUyeOlForEbulten").attr("style", "display:none;");
                    eBultenShowBox('E-Bülten üyelik kaydınız alınmıştır,<br>teşekkür ederiz...', '', 6000);
                }
                else {
                    eBultenShowBox(data.Message, 'uyari', 2500);
                }
            });
        }
    }
    function eBultenShowBox(_message, _class, _delay) {
        $("#lblMessageForEbulten").attr("class", _class);
        $("#lblMessageForEbulten").html(_message);

        $("#uyariBoxForEbulten").show();
        setTimeout("$('#uyariBoxForEbulten').hide();", _delay);
    }
</script>
