<script type="text/javascript">


    $('.vdo-tri > a').click(function(e) {
      e.preventDefault();
      $('.tri-selected').removeClass('tri-selected');
      var self = $(this);
      var tri = self.data('tri');
      self.addClass('tri-selected');

      $('#tri').val(tri);

      $('.vdo-recherche').remove();
      $('#page').val(0);
      $('#totalVideos').val(0);
    });

    $('#form-recherche-smartphone').submit(function(e) {
        e.preventDefault();

        // vidage des videos prcdemment charges et remise  zro
        $('.vdo-recherche').remove();
        $('#page').val(0);
        $('#totalVideos').val(0);

        if ($('#recherche').val() === '' || $('#recherche').val() === 'Nouvelle recherche') return false;

        var datas = {
            univers: $('#univers').val(),
            page: $('#page').val(),
            recherche: $('#recherche').val(),
            tri: $('#tri').val()
        };

        $.ajax({
            url: '/recherche-ajax',
            type: 'POST',
            data: datas,
            dataType: 'json'
        }).then(function(json) {
            $('#totalVideos').val(json.total);
            $('#page').val(1);

            var source = $("#video-template").html();
            var template = Handlebars.compile(source);
            var html = template(json);

            $('.conteneur-recherche').append(html);

            // instanciation d'un infinite scroll une fois la premire recherche effectue
            window.infiniteScrollSearchCatalogue = new FigaroTv.InfiniteScrollV2('vdo-recherche', 15, 'ONGSMARTTH4');
            infiniteScrollSearchCatalogue.init();
        }, function(e) {
            console.log(e);
        });

    });
</script>        </section>

