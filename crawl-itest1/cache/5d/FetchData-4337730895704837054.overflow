	sokbo_num = sokbo_num + 1;
}
function startSokbo(){
	sokbo_time = setTimeout("scrollSokbo() ", 4000);  
}
function stopSokbo(){
	clearTimeout(sokbo_time);  
}
if(document.getElementById("b_news") != null){
	scrollSokbo();
}
</script> 

