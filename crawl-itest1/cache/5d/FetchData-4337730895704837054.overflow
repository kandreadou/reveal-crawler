	var str = "<b>속보</b><a href='" + BreakNews_data[sokbo_num][0] + "' onmouseover='stopSokbo()' onmouseout='startSokbo()' onclick='startSokbo()'>" + BreakNews_data[sokbo_num][1] + "</a>";
